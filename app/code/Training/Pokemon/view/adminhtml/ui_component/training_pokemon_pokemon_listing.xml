<?xml version="1.0" ?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <argument name="data" xsi:type="array">
        <item name="js_config" xsi:type="array">
            <item name="provider" xsi:type="string">training_pokemon_pokemon_listing.training_pokemon_pokemon_listing_data_source</item>
        </item>
    </argument>
    <settings>
        <deps>
            <dep>training_pokemon_pokemon_listing.training_pokemon_pokemon_listing_data_source</dep>
        </deps>
        <spinner>training_pokemon_pokemon_columns</spinner>
    </settings>
    <dataSource name="training_pokemon_pokemon_listing_data_source" component="Magento_Ui/js/grid/provider">
        <dataProvider name="training_pokemon_pokemon_listing_data_source" class="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
            <settings>
                <requestFieldName>id</requestFieldName>
                <primaryFieldName>id</primaryFieldName>
            </settings>
        </dataProvider>
    </dataSource>
    <columns name="training_pokemon_pokemon_columns">
        <column name="id">
            <settings>
                <label translate="true">ID</label>
            </settings>
        </column>
        <column name="name">
            <settings>
                <label translate="true">Name</label>
            </settings>
        </column>
        <column name="url">
            <settings>
                <label translate="true">URL</label>
            </settings>
        </column>
        <column name="is_enable">
            <settings>
                <label translate="true">Is Enable</label>
            </settings>
        </column>
    </columns>
</listing>
